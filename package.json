{
  "name": "sombra",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "node -e \"require('fs').writeFileSync('node_modules/.package-lock-hash',require('crypto').createHash('sha256').update(require('fs').readFileSync('package-lock.json')).digest('hex'))\"",
    "check-deps": "node -e \"const f=require('fs'),c=require('crypto');const want=c.createHash('sha256').update(f.readFileSync('package-lock.json')).digest('hex');let have;try{have=f.readFileSync('node_modules/.package-lock-hash','utf8')}catch{}if(want!==have){console.error('node_modules is out of sync with package-lock.json.\\nRun: rm -rf node_modules && npm install');process.exit(1)}\"",
    "predev": "npm run check-deps",
    "prebuild": "npm run check-deps",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@xyflow/react": "^12.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.571.0",
    "radix-ui": "^1.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-resizable-panels": "^4.6.4",
    "tailwind-merge": "^3.4.1",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react-swc": "^4.2.3",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "shadcn": "^3.8.5",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
